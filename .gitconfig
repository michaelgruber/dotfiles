[user]
	name = Michael Gruber
	email = mail@michaelgruber.me
[core]
	editor = "vim -f"
	excludesfile = ~/.gitignore_global
[color]
	ui = true
[merge]
	tool = vimdiff
[alias]
	unstage = reset HEAD --
	last = log -1 HEAD
	bundled = !sh ~/.bin/git_bundled.sh
[cor]
	autocrlf = false
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[push]
	default = simple
[github]
	user = michaelgruber
